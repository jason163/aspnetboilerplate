<?xml version="1.0" encoding="utf-8"?>
<SQLConfig xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <SQLList>
    <!-- 分页查询渠道品牌信息 -->
    <SQL SQLKey="QueryChannelBrandList" ConnectionKey="Read">
      <Text>
        <![CDATA[
    SELECT COUNT(1) AS TotalCount
      FROM ECMaster.dbo.ChannelBrand CB WITH(NOLOCK)
      LEFT JOIN [ECMaster].[dbo].[Brand] B WITH(NOLOCK)
      ON CB.BrandSysNo = B.SysNo
      LEFT JOIN [ECMaster].[dbo].[Channel] C WITH(NOLOCK)
      ON CB.ChannelID = C.ChannelID
      #STRWHERE#
        
    SELECT TOP @PageSize rownumber
                ,SysNo
                ,ChannelID
                ,ChannelName
                ,BrandSysNo
                ,BrandName
                ,MappingBrandID
                ,MappingBrandName
                ,InUserSysNo
                ,InUserName
                ,InDate
                ,EditUserSysNo
                ,EditUserName
                ,EditDate	

    FROM
    (
      SELECT 
        ROW_NUMBER() over(order by @SortFields) as rownumber
                ,CB.SysNo
                ,CB.ChannelID
                ,C.ChannelName
                ,CB.BrandSysNo
                ,B.Name AS BrandName
                ,CB.MappingBrandID
                ,CB.MappingBrandName
                ,CB.InUserSysNo
                ,CB.InUserName
                ,CB.InDate
                ,CB.EditUserSysNo
                ,CB.EditUserName
                ,CB.EditDate	
      FROM ECMaster.dbo.ChannelBrand CB WITH(NOLOCK)
      LEFT JOIN [ECMaster].[dbo].[Brand] B WITH(NOLOCK)
      ON CB.BrandSysNo = B.SysNo
      LEFT JOIN [ECMaster].[dbo].[Channel] C WITH(NOLOCK)
      ON CB.ChannelID = C.ChannelID
        #STRWHERE#
      ) T WHERE rownumber > (@PageIndex * @PageSize) 
            ]]>
      </Text>
    </SQL>
    <!-- 选择渠道品牌查询品牌列表 -->
    <SQL SQLKey="SelectChannelBrandQuery" ConnectionKey="Read">
      <Text>
        <![CDATA[
    SELECT COUNT(1) AS TotalCount
      FROM ECMaster.dbo.Brand WITH(NOLOCK)
      #STRWHERE#        
        
    SELECT TOP @PageSize rownumber
	                      ,SysNo
                ,Name
                ,NameEN
                ,JianPin
                ,CommonStatus
                ,Summary
                ,Description
                ,DefaultImage
                ,ManufactureSysNo
                ,InUserSysNo
                ,InUserName
                ,InDate
                ,EditUserSysNo
                ,EditUserName
                ,EditDate

    FROM
    (
      SELECT 
        ROW_NUMBER() over(order by @SortFields) as rownumber
                        ,SysNo
                ,Name
                ,NameEN
                ,JianPin
                ,CommonStatus
                ,Summary
                ,Description
                ,DefaultImage
                ,ManufactureSysNo
                ,InUserSysNo
                ,InUserName
                ,InDate
                ,EditUserSysNo
                ,EditUserName
                ,EditDate
	
      FROM ECMaster.dbo.Brand WITH(NOLOCK)
        #STRWHERE#
      ) T WHERE rownumber > (@PageIndex * @PageSize) 
            ]]>
      </Text>
    </SQL>
    <!-- 根据渠道品牌系统编号获取渠道品牌信息 -->
    <SQL SQLKey="LoadChannelBrand" ConnectionKey="Read">
      <Text>
        <![CDATA[
  SELECT CB.[SysNo]
      ,CB.[ChannelID]
      ,CB.[BrandSysNo]
      ,CB.[MappingBrandID]
      ,CB.[MappingBrandName]
      ,CB.[InUserSysNo]
      ,CB.[InUserName]
      ,CB.[InDate]
      ,CB.[EditUserSysNo]
      ,CB.[EditUserName]
      ,CB.[EditDate]
      ,C.[ChannelName]
      ,B.[Name] AS BrandName
  FROM [ECMaster].[dbo].[ChannelBrand](NOLOCK) CB
  LEFT JOIN [ECMaster].[dbo].[Channel](NOLOCK) C
    ON CB.[ChannelID] = C.[ChannelID]
  LEFT JOIN [ECMaster].[dbo].[Brand](NOLOCK) B
    ON CB.[BrandSysNo] = B.[SysNo]
  WHERE CB.[SysNo] = @SysNo        
        ]]>
      </Text>
    </SQL>
    <!-- 获取渠道品牌信息 -->
    <SQL SQLKey="GetChannelBrand1" ConnectionKey="Read">
      <Text>
        <![CDATA[
  SELECT CB.[SysNo]
      ,CB.[ChannelID]
      ,CB.[BrandSysNo]
      ,CB.[MappingBrandID]
      ,CB.[MappingBrandName]
      ,CB.[InUserSysNo]
      ,CB.[InUserName]
      ,CB.[InDate]
      ,CB.[EditUserSysNo]
      ,CB.[EditUserName]
      ,CB.[EditDate]
      ,C.[ChannelName]
      ,B.[Name] AS BrandName
  FROM [ECMaster].[dbo].[ChannelBrand](NOLOCK) CB
  LEFT JOIN [ECMaster].[dbo].[Channel](NOLOCK) C
    ON CB.[ChannelID] = C.[ChannelID]
  LEFT JOIN [ECMaster].[dbo].[Brand](NOLOCK) B
    ON CB.[BrandSysNo] = B.[SysNo]
  WHERE CB.[ChannelID] = @ChannelID
  AND CB.[MappingBrandID] = @MappingBrandID
        ]]>
      </Text>
    </SQL>
    <!-- 获取渠道品牌信息 -->
    <SQL SQLKey="GetChannelBrand2" ConnectionKey="Read">
      <Text>
        <![CDATA[
  SELECT CB.[SysNo]
      ,CB.[ChannelID]
      ,CB.[BrandSysNo]
      ,CB.[MappingBrandID]
      ,CB.[MappingBrandName]
      ,CB.[InUserSysNo]
      ,CB.[InUserName]
      ,CB.[InDate]
      ,CB.[EditUserSysNo]
      ,CB.[EditUserName]
      ,CB.[EditDate]
      ,C.[ChannelName]
      ,B.[Name] AS BrandName
  FROM [ECMaster].[dbo].[ChannelBrand](NOLOCK) CB
  LEFT JOIN [ECMaster].[dbo].[Channel](NOLOCK) C
    ON CB.[ChannelID] = C.[ChannelID]
  LEFT JOIN [ECMaster].[dbo].[Brand](NOLOCK) B
    ON CB.[BrandSysNo] = B.[SysNo]
  WHERE CB.[ChannelID] = @ChannelID
  AND CB.[MappingBrandID] = @MappingBrandID
  AND CB.[SysNo] <> @SysNo
        ]]>
      </Text>
    </SQL>
    <!-- 保存渠道品牌信息 -->
    <SQL SQLKey="SaveChannelBrand" ConnectionKey="Write">
      <Text>
        <![CDATA[
IF(EXISTS(SELECT 1 FROM [ECMaster].[dbo].[ChannelBrand](NOLOCK)
    WHERE [ChannelID] = @ChannelID AND [BrandSysNo] = @BrandSysNo))
BEGIN
	UPDATE [ECMaster].[dbo].[ChannelBrand]
		SET [MappingBrandID] = @MappingBrandID
			,[MappingBrandName] = @MappingBrandName
			,[EditUserSysNo] = @EditUserSysNo
			,[EditUserName] = @EditUserName
			,[EditDate] = GETDATE()
	WHERE [ChannelID] = @ChannelID
	AND [BrandSysNo] = @BrandSysNo
END
ELSE
BEGIN
	INSERT INTO [ECMaster].[dbo].[ChannelBrand]
           ([ChannelID]
           ,[BrandSysNo]
           ,[MappingBrandID]
           ,[MappingBrandName]
           ,[InUserSysNo]
           ,[InUserName]
           ,[InDate]
           ,[EditUserSysNo]
           ,[EditUserName]
           ,[EditDate])
     VALUES
           (@ChannelID
           ,@BrandSysNo
           ,@MappingBrandID
           ,@MappingBrandName
           ,@InUserSysNo
           ,@InUserName
           ,GETDATE()
           ,NULL
           ,NULL
           ,NULL)
END
        ]]>
      </Text>
    </SQL>
    <!-- 更新渠道品牌信息 -->
    <SQL SQLKey="UpdateChannelBrand" ConnectionKey="Write">
      <Text>
        <![CDATA[
	UPDATE [ECMaster].[dbo].[ChannelBrand]
		SET [MappingBrandID] = @MappingBrandID
			,[MappingBrandName] = @MappingBrandName
			,[EditUserSysNo] = @EditUserSysNo
			,[EditUserName] = @EditUserName
			,[EditDate] = GETDATE()
	WHERE [SysNo] = @SysNo
        ]]>
      </Text>
    </SQL>
    <!-- 删除渠道品牌信息 -->
    <SQL SQLKey="DeleteChannelBrand" ConnectionKey="Write">
      <Text>
        <![CDATA[
	DELETE [ECMaster].[dbo].[ChannelBrand]
	WHERE [SysNo] = @SysNo
        ]]>
      </Text>
    </SQL>
  </SQLList>
</SQLConfig>